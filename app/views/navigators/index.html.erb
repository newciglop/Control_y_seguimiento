<p id="notice"><%= notice %></p>

<h1>Navigators</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @navigators.each do |navigator| %>
      <tr>
        <td><%= link_to 'Show', navigator %></td>
        <td><%= link_to 'Edit', edit_navigator_path(navigator) %></td>
        <td><%= link_to 'Destroy', navigator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Navigator', new_navigator_path %>
